{
  "name": "play-ts-cucumber",
  "version": "1.0.0",
  "description": "LPB Automation Test Results",
  "main": "index.js",
  "scripts": {
    "pretest": "npx ts-node src/helper/report/init.ts",
    "test": "cross-env ENV=test FORCE_COLOR=0 cucumber-js --config=config/cucumber.js || true",
    "posttest": "npx ts-node src/helper/report/report.ts",
    "test:failed": "cucumber-js -p rerun @rerun.txt",
    "test:sequence": "npx playwright test tests/sequence/"
  },
  "keywords": [
    "cucumber",
    "cucumber - TS",
    "playwright-cucumber"
  ],
  "author": "Quest QA Team",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.50.1",
    "@types/mssql": "^9.1.1",
    "@types/node": "^20.19.17",
    "@types/pdf-parse": "^1.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.3",
    "mssql": "^10.0.1",
    "multiple-cucumber-html-reporter": "^3.3.0",
    "playwright": "^1.39.0",
    "playwright-core": "^1.36.2",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "winston": "^3.8.2"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "browsers": "^1.0.2",
    "color-name": "^2.0.0",
    "exceljs": "^4.4.0",
    "faker": "^5.5.3",
    "flatted": "^3.3.1",
    "fs": "^0.0.1-security",
    "jsondiffpatch": "^0.5.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "qs": "^6.11.2",
    "tinycolor2": "^1.6.0",
    "util": "^0.12.5",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0"
  }
}
